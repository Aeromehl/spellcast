<variables>
	<var name = "TPammo">Iron Bullet</var><!-- TP Bullets GO HERE -->
	<var name = "WSammo">Iron Bullet</var><!-- WS ACC Bullets GO HERE -->
	<var name = "WSMABammo">Bronze Bullet</var><!-- WS MAB Bullets GO HERE -->
	<var name = "SHOTammo">Orichalc. Bullet</var><!-- SHOT Bullets GO HERE -->
	<var name = "BARRAGEammo">Bronze Bullet</var><!-- BARRAGE Bullets GO HERE -->
	<var name = "Autoshoot">0</var><!-- AutoShoot Variable 1=yes 2=no -->
</variables>
<rules>
	
	
	
			<action type = "equip" when = "Engaged" set = "Melee"/>
			<action type = "equip" when = "Idle" set = "Idle"/>		

<!-- AutoShoot Rules (For use with "var name Autoshoot") -->
		<if advanced = ' "%status" = "engaged" AND "$Autoshoot" = "1"'>
			<if CommandPrefix = "/ra|/range|/ranged">
                <action type = "command" when = "aftercast">wait 9;input /range &lt;t&gt;</action>
			</if>	
        </if>
<!-- Engaged Rules -->				
	<if status = "Engaged">			
			
			<if CommandPrefix = "/ra|/range|/ranged">		
				<if buffactive="Barrage">
					<action type = "equip" when = "precast" set = "BarrageGun"/>
					<command when = "aftercast">wait 3; input  //sc s Melee</command>
					<!--<action type = "equip" when = "aftercast" set = "Melee"/>--> 
				</if>
				<else>
					<action type = "equip" when = "precast" set = "TPgun"/>
					<command when = "aftercast">wait 3; input  //sc s Melee</command>
					<!--<action type = "equip" when = "aftercast" set = "Melee"/>--> 
				</else>
			</if>	
			<if Spell = "Slug Shot|Detonator|Hot Shot|Split Shot|Sniper Shot">
				<action type ="equip" when ="precast" set ="WSgun"/>
				<action type ="equip" when ="aftercast" set ="Melee"/> 	
			</if>
			<if Spell = "Leaden Salute|Wildfire">
				<action type ="equip" when ="precast" set ="MABgun"/>
				<action type ="equip" when ="aftercast" set ="Melee"/> 	
			</if>
			<if Type = "CorsairShot|Corsair Shot|Quick Draw">
				<action type ="equip" when ="precast" set ="SHOTgun"/>
				<action type ="equip" when ="aftercast" set ="Melee"/> 	
			</if>
			
	</if>	
<!-- Not Engaged Rules(not engaged) -->	
	<else>
			
			<if CommandPrefix = "/ra|/range|/ranged">
				<if buffactive = "Barrage">
					<action type = "equip" when = "precast" set = "BarrageGun"/>
					<command when = "aftercast">wait 3; input  //sc s Idle</command>
					<!--<action type = "equip" when = "aftercast" set = "Melee"/>--> 
				</if>
				<else>
					<action type = "equip" when = "precast" set = "TPgun"/>
					<command when = "aftercast">wait 3; input  //sc s Idle</command>
					<!--<action type = "equip" when = "aftercast" set = "Melee"/>--> 
				</else>
			</if>	
			<if Spell = "Slug Shot|Detonator|Hot Shot|Split Shot|Sniper Shot">
				<action type ="equip" when ="precast" set ="WSgun"/>
				<action type ="equip" when ="aftercast" set ="Idle"/> 	
			</if>
			<if Spell = "Leaden Salute|Wildfire">
				<action type ="equip" when ="precast" set ="MABgun"/>
				<action type ="equip" when ="aftercast" set ="Idle"/> 	
			</if>
			<if Type = "CorsairShot|Corsair Shot|Quick Draw">
				<action type ="equip" when ="precast" set ="SHOTgun"/>
				<action type ="equip" when ="aftercast" set ="Idle"/> 	
			</if>
		
	</else>		

	
