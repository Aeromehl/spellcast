<rules>
	<!-- Status Rules -->	
				<action type="equip" when="Idle|Resting" set="DNCIdle"/>
				<action type="equip" when="Engaged" set="DNCTP"/>
	
	<!-- Aftercast Rules -->
			<if status="Engaged">
				<action type="equip" when="Aftercast" set="DNCTP"/>
			</if>
			<else>
				<action type="equip" when="Aftercast" set="DNCIdle"/>
			</else>	
	
	<!-- WS Rules -->
			<if commandprefix="/ws|/weaponskill">
				<if spell="Exenterator">
					<action type="equip" when="Precast" Set="DNCagiWS" />
				</if>	
				<else>
					<action type="equip" when="Precast" Set="DNCWS" /> 
				</else>	
			</if>
	
	<!-- Wasp Sting > Exenterator -->
			<if Spell="Wasp Sting">
							<action Type="ChangeSpell" Spell="Exenterator"/>
			</if>			
			
	<!-- Spell Rules -->	
			<if spell="*Jig|* Jig">
				<action type="equip" when="Precast" set="DNCjig"/>
			</if>
			
			<if spell="*Waltz|* Waltz">
				<action type="equip" when="Precast" set="DNCwaltz"/>
			</if>
		
				 
			<if spell="Utsusemi: Ichi" BuffActive="Copy Image">
				<action type="midcastdelay" delay="3.0" />
				<action type="Command" when="midcast">cancel 66</action>
			</if>
</rules>
